{
  "manifest_version": 3,
  "name": "LinkedIn Helper",
  "version": "1.0",
  "description": "Turns all websites black and white.",
  "permissions": [
    "activeTab"
  ],

  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}